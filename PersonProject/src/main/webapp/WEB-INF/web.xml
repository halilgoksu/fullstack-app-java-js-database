<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>


  <servlet>
    <servlet-name>staff</servlet-name>
    <servlet-class>com.revature.CreateLogin.controller.PersonServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>staff</servlet-name>
    <!-- we include the * meaning anything can come after this -->
    <url-pattern>/staff</url-pattern>
  </servlet-mapping>


<servlet>
  <servlet-name>ticket</servlet-name>
  <servlet-class>com.revature.Ticket.controller.TicketServlet</servlet-class>
</servlet>
  <servlet-mapping>
    <servlet-name>ticket</servlet-name>
    <!-- we include the * meaning anything can come after this -->
    <url-pattern>/ticket</url-pattern>
  </servlet-mapping>


</web-app>
